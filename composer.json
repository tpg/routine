{
    "name": "thepublicgood/routine",
    "description": "Class-based routing for Laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TPG\\Routine\\": "src/"
        }
    },
    "authors": [
        {
            "name": "THEPUBLICGOOD",
            "email": "makers@thepublicgood.dev",
            "role": "Developer",
            "homepage": "https://thepublicgood.dev"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "pestphp/pest": "^1.21",
        "orchestra/testbench": "^7.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TPG\\Routine\\RoutineServiceProvider"
            ]
        }
    }
}
